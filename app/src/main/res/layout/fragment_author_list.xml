<?xml version="1.0" encoding="utf-8"?><com.github.florent37.parallax.ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:parallaxVertical="0.3">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:theme="@style/AppTheme.NoActionBar">

        <include layout="@layout/toolbar_layout" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="54dp"
        android:orientation="vertical">

        <com.github.florent37.parallax.ParallaxView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:parallaxVertical="0.5">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/rec_fragment_try_test_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"></android.support.v7.widget.RecyclerView>
        </com.github.florent37.parallax.ParallaxView>
    </LinearLayout>
</RelativeLayout>
</com.github.florent37.parallax.ScrollView>